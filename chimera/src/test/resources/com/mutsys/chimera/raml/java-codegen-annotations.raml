#%RAML 1.0 Library
usage: |
  annotations for declaring java type metadata on your
  raml types to customize java code generation

annotationTypes:
  javaClassType:
    allowedTargets: [ TypeDeclaration, Resource ]
    type: string
    enum: ['interface','abstractClass','concreteClass']
  javaClassName:
    allowedTargets: [ TypeDeclaration, Resource ]
    type: string
  javaPackageName:
    allowedTargets: [ TypeDeclaration, Resource ]
    type: string